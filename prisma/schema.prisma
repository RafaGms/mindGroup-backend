datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id       Int     @id @default(autoincrement())
  name     String
  email    String  @unique
  password String
  Images   Image[]
}

model Image {
  id     Int    @id @default(autoincrement())
  path   String
  User   User   @relation(fields: [userId], references: [id])
  userId Int
}
